{
  "kind": "collectionType",
  "collectionName": "metadata",
  "info": {
    "name": "Metadata",
    "description": "Metadata contains page-based SEO labels that are bound to a specific page"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "label": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "maxLength": 60
    },
    "description": {
      "type": "string",
      "maxLength": 160
    },
    "heroImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "structuredData": {
      "type": "json"
    },
    "label1": {
      "type": "string"
    },
    "data1": {
      "type": "string"
    },
    "label2": {
      "type": "string"
    },
    "data2": {
      "type": "string"
    },
    "extendedTitle": {
      "type": "string"
    }
  }
}
