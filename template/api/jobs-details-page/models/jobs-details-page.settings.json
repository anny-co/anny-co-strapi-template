{
  "kind": "singleType",
  "collectionName": "jobs_details_pages",
  "info": {
    "name": "JobsDetailsPage"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "enableChat": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "elements": {
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.feature-row-group",
        "sections.feature-column-group",
        "sections.pricing",
        "sections.rich-text",
        "sections.testimonial-group",
        "sections.partners",
        "sections.main-feature",
        "sections.call-to-action",
        "sections.section-reference",
        "sections.header-section",
        "sections.team",
        "sections.media-section",
        "sections.newsletter-section",
        "sections.faq-section",
        "sections.banner-section",
        "sections.component-slot",
        "sections.table-section",
        "sections.gallery",
        "sections.embedded-content-section",
        "sections.full-width-section"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "navigation": {
      "model": "navigations"
    },
    "footer": {
      "model": "footers"
    },
    "include": {
      "collection": "sections"
    },
    "metadata": {
      "model": "metadata"
    }
  }
}
