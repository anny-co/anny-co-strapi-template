{
  "kind": "collectionType",
  "collectionName": "navigations",
  "info": {
    "singularName": "navigation",
    "pluralName": "navigations",
    "displayName": "Navigations",
    "description": "Navigations contains instances of a navigation such that we can exchange which page uses which navigation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "logoUrl": {
      "type": "text",
      "default": "/",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "elements": {
      "type": "dynamiczone",
      "components": [
        "elements.navigation-item",
        "links.link",
        "links.button",
        "layout.spacer"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "theme": {
      "type": "component",
      "repeatable": false,
      "component": "layout.navigation-theme",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
