{
  "collectionName": "components_elements_medias",
  "info": {
    "displayName": "Media",
    "description": "A Media element wraps a strapi media object and adds styling and configurability to it",
    "icon": "brush"
  },
  "options": {},
  "attributes": {
    "media": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "shadow": {
      "type": "boolean",
      "default": true
    },
    "roundedCorners": {
      "type": "boolean",
      "default": true
    },
    "style": {
      "type": "json"
    },
    "embeddedContent": {
      "type": "component",
      "repeatable": false,
      "component": "elements.embedded-content"
    }
  }
}
